{
	"title": "添加订单",
	"path": "/order/add",
	"query": {
		"url": "/order/add",
		"method": "post",
		"params": [{
			"key": "orderId",
			"type": "input",
      "templateOptions": {
        "type": "text",
        "label": "订单号",
        "placeholder": "输入订单号",
        "required": true
      }
		}, {
			"type": "select",
			"key": "orderType",
			"templateOptions": {
				"label": "订单类型",
				"options": [{
					"value": 0,
					"name": "全部"
				}, {
					"value": 1,
					"name": "线上订单"
				}]
			}
		}]
	}
}